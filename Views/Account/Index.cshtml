@model IEnumerable<Account>
@{
    ViewData["Title"] = "Index";
     Layout = "~/Views/Shared/_HomePage.cshtml";
}

<table>
    <thead>
        <tr>
           
            <th>Name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
               
                <td>@item.Username</td>
                <td>@item.Password</td>
            </tr>
        }

    </tbody>
</table>